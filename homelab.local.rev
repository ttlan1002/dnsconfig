$TTL 86400
@ IN SOA dhcpdns01.homelab.local. admin.homelab.local. (
				3 ;Serial
				3600  ;Refresh
				1800 ;Retry
				604800 ;Expire
				86400; Minimum TTL
)
;Name Server Info
@ IN NS dhcpdns01.homelab.local.

dhcpdns01 IN A 192.168.1.110

110 IN PTR dhcpdns01.homelab.local.
150 IN PTR ansible.homelab.local.
120 IN PTR satellite.homelab.local.
130 IN PTR newsatellite.homelab.local.
115 IN PTR capsule.homelab.local.
